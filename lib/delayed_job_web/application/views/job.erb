<table id="tblData" class="table table-bordered">
  <thead>
    <tr>
      <th><input type="checkbox" id="chkParent"/></th></th>
      <th>ID</th>
      <th>Priority</th>
      <th>Attempts</th>
      <th>Queue</th>
      <th>Handler</th>
      <th>Run At</th>
      <th>Updated At</th>
      <th>Created At</th>
      <th>Locked At</th>
      <th>Locked By</th>
      <th>Last Error</th>
      <th>Failed At</th>
      <th>Tags</th>
    </tr>
  </thead>
  <tbody>
    <% if @jobs.any? %>
      <% @jobs.each do |job| %>
        <tr>
          <td><input type="checkbox" name="jobid" value=<%= job.id %>></td>
          <td><%= job.id %></td>
          <td><%= job.priority%></td>
          <td><%= job.attempts %></td>
          <td><%= job.queue %></td>
          <td><%= job.handler.slice(0..19) + "..." %></td>
          <td><%= job.run_at %></td>
          <td><%= job.updated_at %></td>
          <td><%= job.created_at %></td>
          <td><%= job.locked_at %></td>
          <td><%= job.locked_by %></td>
          <td><%= job.last_error.slice(0..19) + "..." if job.last_error%></td>
          <td><%= job.failed_at %></td>
          <td><%= job.attributes["tags"].join(", ") if job.attributes["tags"]%></td>
        </tr>
      <% end %>
    <% else %>
      <tr>
        <td colspan="13">
          <p>No Jobs</p>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
